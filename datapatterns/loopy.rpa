abra	::= [0-9]+
#cadabra ::= <:abra:>abc

n	::= [0-9]+ | '(' <:land:> ')'

plus	::= <:plus:> '+' <:n:> | <:n:>
minus	::= <:minus:> '-' <:plus:> | <:plus:>
mul	::= <:mul:> '*' <:minus:> | <:minus:>
div	::= <:div:> '/' <:mul:> | <:mul:>
or	::= <:or:> '|' <:div:> | <:div:>
xor	::= <:xor:> '^' <:or:> | <:or:>
and	::= <:and:> '&' <:xor:> | <:xor:>
at	::= <:and:> '@' <:and:> | <:and:>
mod	::= <:mod:> '%' <:at:> | <:at:>
land	::= <:land:> '&&' <:mod:> | <:mod:>

all	::= <:land:>
